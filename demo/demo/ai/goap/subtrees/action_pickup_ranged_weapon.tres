[gd_resource type="BehaviorTree" load_steps=5 format=3]

[ext_resource type="Script" path="res://demo/ai/goap/tasks/goap_move_to.gd" id="1_moveto"]
[ext_resource type="Script" path="res://demo/ai/goap/tasks/goap_pickup.gd" id="2_pickup"]

[sub_resource type="BTAction" id="BTAction_moveto"]
script = ExtResource("1_moveto")
target_var = &"ranged_weapon_pickup"
tolerance = 80.0
speed = 300.0

[sub_resource type="BTAction" id="BTAction_pickup"]
script = ExtResource("2_pickup")
pickup_var = &"ranged_weapon_pickup"
result_var = &"has_weapon"

[sub_resource type="BTSequence" id="BTSequence_1"]
children = [SubResource("BTAction_moveto"), SubResource("BTAction_pickup")]

[resource]
description = "GOAP Action: Move to ranged weapon and pick it up"
root_task = SubResource("BTSequence_1")
